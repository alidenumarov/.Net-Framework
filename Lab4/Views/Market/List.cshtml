@using System.Linq
@{
    ViewData["Title"] = "List of Markets";
}
<h1>List of Markets</h1>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Rating</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.MarketName</td>
                <td>@item.Rating</td>
            </tr>
        }
    </tbody>
</table>